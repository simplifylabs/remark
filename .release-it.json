{
  "hooks": {
    "22222before:github:release": "rm -rf dist/ && yarn dist"
  },
  "git": {
    "commitMessage": "Release v${version}",
    "changelog": null,
    "tagAnnotation": "v${version}",
    "tagName": "v${version}",
    "requireCleanWorkingDir": true,
    "tag": true,
    "commit": true,
    "push": true
  },
  "github": {
    "release": true,
    "autoGenerate": true,
    "releaseName": "v${version}",
    "assets": ["dist/*.(zip)"],
    "web": false
  },
  "npm": {
    "publish": false
  }
}
