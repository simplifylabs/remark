{
  "root": "apps/cdn",
  "sourceRoot": "apps/cdn/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/node:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "apps/cdn/dist/",
        "main": "apps/cdn/src/app.ts",
        "tsConfig": "apps/cdn/tsconfig.json",
        "assets": []
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/cdn/src/environments/environment.ts",
              "with": "apps/cdn/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/node:execute",
      "options": {
        "buildTarget": "cdn:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/cdn/**/*.ts"]
      }
    }
  },
  "tags": []
}
